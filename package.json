{
  "name": "solarity-extension",
  "version": "1.0.0",
  "description": "A chrome extension for Solarity built with Tailwind 3, React 17, Webpack 5, and Webpack Dev Server 4",
  "license": "MIT",
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx"
    }
  },
  "scripts": {
    "watch": "npm-watch",
    "build": "node utils/build.js",
    "start": "node utils/webserver.js",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "tailwind": "npx tailwindcss -o ./src/assets/styles/tailwind.css --minify"
  },
  "dependencies": {
    "@nfteyez/sol-rayz": "^0.10.2",
    "@reduxjs/toolkit": "^1.8.3",
    "@solana/web3.js": "1.29.1",
    "@types/async": "^3.2.13",
    "axios": "^0.27.2",
    "blob-to-buffer": "^1.2.9",
    "bluebird": "^3.7.2",
    "buffer": "^6.0.3",
    "font-awesome": "^4.7.0",
    "ipfs": "^0.62.3",
    "jquery": "^3.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^14.2.1",
    "react-redux": "7.2.8",
    "react-rnd": "^10.3.7",
    "react-textarea-autosize": "^8.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@hot-loader/react-dom": "^17.0.2",
    "@types/chrome": "^0.0.177",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "assert": "^2.0.0",
    "autoprefixer": "^10.4.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "babel-preset-react-app": "^10.0.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^6.6.0",
    "eslint": "^8.8.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.0.0",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "https-browserify": "^1.0.0",
    "node-sass": "^6.0.1",
    "postcss": "^8.4.6",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.1",
    "react-hot-loader": "^4.13.0",
    "sass-loader": "^12.4.0",
    "source-map-loader": "^3.0.1",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.0.19",
    "terser-webpack-plugin": "^5.3.1",
